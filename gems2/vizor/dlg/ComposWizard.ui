            <widget class="QSpinBox">
                <property name="name">
                    <cstring>spinBox18</cstring>
                </property>
                <property name="geometry">
                    <rect>
                        <x>10</x>
                        <y>20</y>
                        <width>60</width>
                        <height>21</height>
                    </rect>
                </property>
                <property name="maxValue">
                    <number>4</number>
                </property>
            </widget>
        </widget>
